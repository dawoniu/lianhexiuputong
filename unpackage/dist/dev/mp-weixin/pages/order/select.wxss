.main .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-left: 5px;
  line-height: 50px;
  height: 50px;
}
.main .item .icon {
  width: 30px;
}
.main .item .icon image {
  width: 18px;
